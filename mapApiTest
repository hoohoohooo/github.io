<script>
  const myKeysValues = window.location.search;
  console.log("k & v: ",myKeysValues);
  const urlParams = new URLSearchParams(myKeysValues);
  console.log(urlParams.get('name'));
  document.write(urlParams.get('name') + '<br>'); 
</script>

